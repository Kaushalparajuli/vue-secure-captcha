{
  "name": "vue-secure-captcha",
  "version": "1.0.8",
  "private": false,
  "description": "A secure, customizable CAPTCHA component for Vue.js",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src --ext .js,.vue",
    "prepare": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "captcha",
    "security",
    "verification",
    "vue3",
    "component"
  ],
  "author": "Kaushal Parajuli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaushalparajuli/vue-secure-captcha.git"
  },
  "homepage": "https://github.com/Kaushalparajuli/vue-secure-captcha#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.30.0"
  }
}
